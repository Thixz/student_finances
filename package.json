{
  "name": "student_finances",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest run --dir src/modules/",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:watch": "vitest --dir src/modules/",
    "test:ui": "vitest run --ui",
    "test:coverage": "vitest run --coverage",
    "dev": "tsx watch --inspect src/server.ts",
    "dockerdev": "ts-node-dev --inspect --transpile-only --poll --ignore-watch node_modules --respawn -r tsconfig-paths/register src/server.ts",
    "prepare-database": "npx prisma generate && npx prisma migrate deploy",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.16",
    "@vitest/coverage-v8": "0.32.2",
    "npm-run-all": "4.1.5",
    "@vitest/ui": "0.32.2",
    "prisma": "6.5.0",
    "supertest": "6.3.3",
    "@types/supertest": "2.0.12",
    "ts-node-dev": "2.0.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "11.0.1",
    "@fastify/jwt": "9.1.0",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.2",
    "@prisma/client": "6.5.0",
    "bcryptjs": "3.0.2",
    "dotenv": "16.4.7",
    "fastify": "5.2.2",
    "fastify-type-provider-zod": "4.0.2",
    "tsconfig-paths": "4.2.0",
    "zod": "3.24.2"
  }
}
